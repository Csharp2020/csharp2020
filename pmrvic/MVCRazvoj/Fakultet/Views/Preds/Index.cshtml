@model IEnumerable<Fakultet.Models.Pred>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.KratPred)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NazPred)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpisanoStud)
