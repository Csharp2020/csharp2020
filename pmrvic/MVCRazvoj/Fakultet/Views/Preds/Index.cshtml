                @Html.DisplayFor(modelItem => item.KratPred)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NazPred)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpisanoStud)
            </td>
